{
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": [
    "google",
    "plugin:react/recommended"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "rules" : {
    "comma-dangle":"off",
    "max-len":"off",
    "require-jsdoc":"off",
    "no-throw-literal": "off"
  }
}